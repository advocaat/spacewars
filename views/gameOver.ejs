<!doctype html>

<html>
<head>
<meta charset="utf-8"/>
<title>game over</title>
<script src="javascripts/model.js"></script>
</head>
<body>
	<h1>Game  Over!!</h1>
	
	<p id="myTime"></p>
	<a href="/">New Game</a>
	<div id="countdown"></div>
	<ul id="scores"></ul>
	<script>
	var myTime = document.getElementById("myTime");
		var time = <%- poof %>;
		addHighScore(time);
		myTime.innerHTML = "lasted " +  time;
		displayHighScores();
	var countdown = document.getElementById("countdown");
		var i = 3;


				setInterval(function(){
				if(i >= -1){
				countdown.innerHTML = "<h2>"+i+"..</h2>";
				console.log("count" + i);
				i-=1;
				if(i == -1){
					location.href="/";

				}


		}
			}, 1000)
			
	

	</script>
	
	
</body>
</html>